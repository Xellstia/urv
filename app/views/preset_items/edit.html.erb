<%= turbo_frame_tag dom_id(@preset_item) do %>
  <% case @preset_item.system.to_s %>
  <% when "yaga" %>
    <%= render "preset_items/form_yaga", preset: @preset, preset_item: @preset_item %>
  <% else %>
    <%= render "preset_items/form_tempo", preset: @preset, preset_item: @preset_item %>
  <% end %>
<% end %>

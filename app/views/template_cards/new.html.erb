<% frame_id = params[:frame_id] || "template_category_form_#{@category.id}" %>

<%= turbo_frame_tag frame_id do %>
  <% if @card.system == "yaga" %>
    <%= render "template_cards/form_yaga", category: @category, card: @card %>
  <% else %>
    <%= render "template_cards/form_tempo", category: @category, card: @card %>
  <% end %>
<% end %>
